{
  "name": "techsite13-careers",
  "version": "1.0.0",
  "description": "Techport13 Careers Website",
  "source": "src/index.html",
  "homepage": "https://careers.techport13.com",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "build-gh": "parcel build src/index.html --no-scope-hoist --public-url './' --dist-dir build",
    "predeploy": "npm run build-gh",
    "deploy": "gh-pages -d build",
    "lint": "eslint \"src/**/*.{js,jsx}\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.{js,jsx,css,html}\""
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.7",
    "gh-pages": "^6.3.0",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10"
  }
}
